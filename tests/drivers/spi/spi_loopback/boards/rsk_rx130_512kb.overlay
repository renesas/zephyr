&rspi0 {
	status = "okay";
	pinctrl-0 = <&rspi0_default>;
	pinctrl-names = "default";
	slow@0 {
		compatible = "test-spi-loopback-slow";
		reg = <0>;
		spi-max-frequency = <1000000>;
	};
	fast@0 {
		compatible = "test-spi-loopback-fast";
		reg = <0>;
		spi-max-frequency = <3000000>;
	};
};
